{
  "name": "tier-discord-bot",
  "description": "Just a Music & Chill Discord Bot",
  "version": "2.0.0",
  "main": "dist/app.js",
  "author": "Vectier",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:dev": "webpack --mode=development --watch",
    "build:prod": "webpack --mode=production",
    "build": "yarn run build:prod",
    "dev": "npm-run-all --silent --parallel build:dev start:dev",
    "lint": "eslint . --ext .ts",
    "start:dev": "nodemon dist/app.js -q -w dist -e js",
    "start:prod": "node dist/app.js",
    "start": "yarn run start:prod",
    "test": "jest"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.31.0",
    "discord.js": "^13.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.3",
    "jest": "^27.5.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "yarn-run-all": "^3.1.1"
  },
  "engines": {
    "node": "^16.14.0",
    "npm": "please-use-yarn"
  }
}
